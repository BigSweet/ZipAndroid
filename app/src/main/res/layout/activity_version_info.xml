<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <include
            android:id="@+id/private_include_title"
            layout="@layout/include_title_bar" />


        <ImageView
            android:id="@+id/version_info_logo"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:layout_marginTop="58dp"
            android:src="@drawable/zip_app_logo"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/private_include_title" />


        <TextView
            android:id="@+id/version_info_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="Ziplead"
            android:textColor="#ff000000"
            android:textSize="19sp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/version_info_logo" />

        <TextView
            android:id="@+id/version_info_version_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:text="Version 1.0.0"
            android:textColor="#ff959db2"
            android:textSize="13sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/version_info_name_tv" />


        <ImageView
            android:layout_width="229dp"
            android:layout_height="224dp"
            android:layout_marginBottom="39dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

        </ImageView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>